{
  "version": 2,
    "builds": [
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api"
      },
      {
        "src": "/",
        "dest": "public/index.html"
      },
      {
        "src": "/api",
        "dest": "public/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "public/$1"
      }
  ]


}
